<Window x:Class="KazuyaFX_StudentSetup.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KazuyaFX_StudentSetup"
        mc:Ignorable="d"
        Title="KazuyaFX Student Setup" Height="300" Width="480">

    <Grid>
        <TabControl x:Name="tabControl" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
            <TabItem Header="Student Settings">
                <local:StudentSettingsControl />
            </TabItem>
        </TabControl>
    </Grid>
</Window>
